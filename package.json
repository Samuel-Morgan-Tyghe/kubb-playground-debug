    },
    "proxy": "https://dev-api.vega.investments/",
    "scripts": {
        "save-translations-type": "i18next-resources-for-ts interface -i ./src/translations -o src/translations/resources.d.ts",
        "save-translations": "node scripts/saveTranslations.mjs",
        "gen:translation": " yarn save-translations && yarn save-translations-type",
        "gen:api-docs": "node scripts/fetchApiDocs.mjs",
        "gen:all": "node scripts/fetchApiDocs.mjs && npx kubb ",
        "prestart": "yarn gen:translation",
        "start": " vite",
        "dev": "vite",
        "build": "vite build && REACTBUILDVERSION=$(date +%d.%m.%Y)-$(date +%T) && echo 'Build Version: ' $REACTBUILDVERSION && sed -i \"s#%REACTBUILDVERSION%#$REACTBUILDVERSION#g\" dist/index.html",
        "preview": "vite preview",
        "serve": "vite serve",
        "test": "TZ=UTC vitest",
        "test:all": "TZ=UTC vitest --run",
        "test:ui": "TZ=UTC vitest --ui",
        "test:watch": "TZ=UTC vitest watch",
        "test:e2e": "TZ=UTC npx playwright test",
        "test:coverage": "TZ=UTC vitest --run --coverage",
        "test:snapshots:update": "TZ=UTC npx playwright test --update-snapshots",
        "test:coverage:badges": "TZ=UTC istanbul-badges-readme --silent",
        "audit:fix": "yarn yarn-audit-fix",
        "audit:check": "yarn audit",
        "lint:all": "eslint --ext .js,.tsx,.ts ./src",
        "lint:all:fix": "eslint ./src --fix  ",
        "lint:gen:fix": "eslint ./src/gen --fix  ",
        "lint:fix:commit": "yarn lint:all --fix",
        "typecheck": "tsc --noEmit",
        "prepare": "husky install",
        "storybook": "storybook dev -p 6006",
        "chromatic": "npx chromatic --project-token=chpt_e809d2178dd2d4f",
        "build-storybook": "storybook build"
    },
    "husky": {
        "hooks": {
            "pre-commit": "echo \"[Husky] pre-commit\" && yarn lint:all:fix"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
